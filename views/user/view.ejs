<div class = "container">
	<h3>Users</h3>
	<table>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Title</th>
			<th>Email</th>

		</tr>
		<% _.each(users, function(user){%>
		<tr data-id="<%=user.id %>" data-model="user">
			<td> <%= user.id %> </td>
			<td> <%= user.name %> </td>
			<td> <%= user.title %> </td>
			<td> <%= user.email %> </td>
			<td><a href="/user/show/<%= user.id %>" class = "waves-effect waves-light btn-large"> Show</a></td>
			<td><a href="/user/edit/<%= user.id %>" class = "waves-effect waves-light btn-large"> Edit</a></td>
			<td>
				<form action="/user/destroy/<%= user.id %>" method="post">
					<input type="hidden" name="_method" value="delete"/>
					<input type="submit" class = "waves-effect waves-light btn-large" value ="Delete"/> 
				</form>
				
			</td>
			<% })%>
	</table>
	

</div>